extends layout
block content
  h2 Registeration

  form(action="/register",method="post")
    div
      label FirstName
      input(type="text",name="firstname")
    
    div
      label LastName
      input(type="text",name="lastname")

    div
      label Email
      input(type="email",name="email")

    div
      label Password
      input(type="password",name="password")

    div
      input(type="submit",value="Register") 